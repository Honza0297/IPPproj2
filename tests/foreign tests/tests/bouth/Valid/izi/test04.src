#ADDING
label _add
createframe
pushframe
defvar LF@op1
defvar LF@op2
pops LF@op1
pops LF@op2
add LF@op1 LF@op1 LF@op2
pushs LF@op1
call !print
popframe
return

#MUTIPLY
label mul
createframe
pushframe
defvar LF@op1
defvar LF@op2
pops LF@op1
pops LF@op2
mul LF@op1 LF@op1 LF@op2
pushs LF@op1
call !print
popframe
return

#DIVIDING STUFFS
label div
createframe
pushframe
defvar LF@op1
defvar LF@op2
pops LF@op1
pops LF@op2
idiv LF@op1 LF@op1 LF@op2
pushs LF@op1
call !print

popframe
return

#SUBTRACTING
label sub
createframe
pushframe
defvar LF@op1
defvar LF@op2
pops LF@op1
pops LF@op2
sub LF@op1 LF@op1 LF@op2
pushs LF@op1
call !print
popframe
return

##PRINTFUNCTION
label !print
createframe
pushframe
defvar LF@stuff
pops LF@stuff
write LF@stuff
write string@\010
popframe
return
